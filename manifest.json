{
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "css": [ "katex.css" ],
      "matches": [ "*://*.discord.com/*"]
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Render LaTeX in Discord chat.",
   "manifest_version": 2,
   "name": "LaTeX in Discord",
   "short_name": "latex-discord",
   "version": "0.5",
   "icons": { "32": "icon32.png",
              "48": "icon48.png",
             "128": "icon128.png" }
}
